{
  "categories": [
    {
      "title": "üöÄ Features",
      "labels": [
        "feature"
      ]
    },
    {
      "title": "üìÉ Documentation",
      "labels": [
        "documentation"
      ]
    },
    {
      "title": "üêõ Bug Fixes ",
      "labels": [
        "bug"
      ]
    }
  ],
  "ignore_labels": [
    "CI",
    "CD",
    "ignore",
    "ignoreEmail"
  ],
  "sort": "ASC",
  "template": "${{CHANGELOG}}\nIf you have any questions about this Release, please feel free to contact me at the email below. \n\n\nYours sincerely\n\nZeng Zihui\ns10195353@connect.np.edu.sg\nProject Manager\nDevOps_Oct2021_T01_Team04",
  "pr_template": "- ${{TITLE}} is done. ",
  "empty_template": "- no changes",
  "label_extractor": [
    {
      "pattern": "(.) (.+)",
      "target": "$1",
      "flags": "gu"
    },
    {
      "pattern": "\\[Issue\\]",
      "on_property": "title",
      "method": "match"
    }
  ],
  "duplicate_filter": {
    "pattern": "\\[ABC-....\\]",
    "on_property": "title",
    "method": "match"
  },
  "transformers": [
    {
      "pattern": "[\\-\\*] (\\[(...|TEST|CI|SKIP)\\])( )?(.+?)\n(.+?[\\-\\*] )(.+)",
      "target": "- $4\n  - $6"
    }
  ],
  "max_tags_to_fetch": 1000,
  "max_pull_requests": 1000,
  "max_back_track_time_days": 1000,
  "exclude_merge_branches": [
    "Owner/qa"
  ],
  "tag_resolver": {
    "method": "semver",
    "filter": {
      "pattern": "api-(.+)",
      "flags": "gu"
    }
  },
  "base_branches": [
    "main"
  ]
}